---
import BaseLayout from "../layouts/BaseLayout.astro";
import Card from "../components/Card.astro";
import RandomIcons from "../components/RandomIcons.astro";
---

<BaseLayout title="About me" description="">
  <!-- Parent container with relative positioning -->
  <div class="min-h-screen relative overflow-hidden">
    <!-- Random Icons with lower z-index (background) -->
    <RandomIcons />

    <!-- Card with higher z-index to ensure it's on top -->
    <div class="relative z-10">
      <Card
        
      />
    </div>
  </div>
</BaseLayout>
